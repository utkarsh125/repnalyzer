datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Document {
  id          Int      @id @default(autoincrement())
  title       String
  content     String
  embedding   Float[]  //might need to change it to native vector type
  createdAt   DateTime @default(now())
}
